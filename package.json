{
  "name": "nu_mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "npm install && cd ios && pod install && cd ..",
    "build-android-debug": "cd ./android && ./gradlew clean && cd .. && npx jetifier && cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "build-android-apk": "cd ./android && ./gradlew clean && cd .. && npx jetifier && cd ./android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@react-native-community/progress-bar-android": "1.0.4",
    "@react-native-community/progress-view": "1.2.3",
    "moment": "2.29.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-animatable": "1.3.3",
    "react-native-device-info": "7.3.1",
    "react-native-navigation": "7.6.0",
    "react-native-pdf": "6.2.2",
    "react-native-print": "0.7.0",
    "react-native-snap-carousel": "3.9.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-vector-icons": "7.1.0",
    "rn-fetch-blob": "0.12.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "1.1.0",
    "babel-jest": "25.1.0",
    "eslint": "6.5.1",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/Montserrat-Bold.ttf"
    ]
  }
}
